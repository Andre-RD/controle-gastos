{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/controle-financeiro",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
